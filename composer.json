{
  "name": "companyos/backend",
  "description": "CompanyOS Backend Bundle - VueJS Frontend for Admin Interface",
  "version": "0.1.136-alpha",
  "type": "symfony-bundle",
  "license": "MIT",
  "keywords": [
    "symfony",
    "bundle",
    "vue",
    "admin",
    "frontend",
    "coreui"
  ],
  "homepage": "https://github.com/companyos/backend",
  "support": {
    "issues": "https://github.com/companyos/backend-bundle/issues",
    "source": "https://github.com/companyos/backend-bundle"
  },
  "authors": [
    {
      "name": "Bechstein.Digital Ecommerce UG",
      "email": "info@bechstein.digital"
    }
  ],
  "require": {
    "php": ">=8.2",
    "symfony/framework-bundle": "^7.3",
    "symfony/twig-bundle": "^7.3",
    "symfony/asset": "^7.3",
    "symfony/webpack-encore-bundle": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "symfony/maker-bundle": "^1.63",
    "symfony/test-pack": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "CompanyOS\\Bundle\\BackendBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CompanyOS\\Bundle\\BackendBundle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.3.*"
    }
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "sort-packages": true
  },
  "minimum-stability": "alpha",
  "prefer-stable": false
}
